package testException;

public class Interface {

	public void verificationCoordonnee(char lettre, int chiffre) {
		if (lettre > 'J')
			System.out.println("lettre trop grande");
		if (chiffre > 10 || chiffre < 1)
			System.out.println("chiffre inférieur à 1 ou supérieur à 10");
	}

	public static void main(String[] args) {
		Interface io = new Interface();
		io.verificationCoordonnee('K', 8);
		io.verificationCoordonnee('B', 11);
	}
}
